{
  "name": "preact-nebula-ui",
  "private": false,
  "version": "1.3.0",
  "type": "module",
  "description": "Modern React component library built with Preact and Tailwind CSS",
  "keywords": [
    "preact",
    "react",
    "components",
    "ui",
    "tailwind",
    "typescript"
  ],
  "author": "PRachwal",
  "license": "MIT",
  "homepage": "https://github.com/PRachwal/preact-nebula-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PRachwal/preact-nebula-ui.git"
  },
  "bugs": {
    "url": "https://github.com/PRachwal/preact-nebula-ui/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run update-coverage && npm run build-docs && tsc -b && vite build && npm run copy-docs-to-dist",
    "build:lib": "npm run update-coverage && npm run build-docs && vite build --mode lib && npm run copy-docs-to-dist",
    "build:lib:fast": "npm run build-docs && npx vite build --mode lib && npm run copy-docs-to-dist",
    "build-docs": "node scripts/build-docs-system.js",
    "copy-docs-to-dist": "node scripts/copy-docs-to-dist.js",
    "move-docs": "node scripts/move-documentation.js",
    "generate-metadata": "node scripts/build-docs-system.js",
    "cleanup-old-docs": "node scripts/cleanup-old-docs.js",
    "update-readmes": "node scripts/update-component-readmes.js",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "update-coverage": "node scripts/update-coverage.js",
    "coverage:json": "vitest run --coverage --coverage.reporter=json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build:lib && npm run test",
    "publish:dry": "npm publish --dry-run",
    "publish:npm": "node scripts/fast-npm-publish.cjs",
    "publish:no-browser": "node scripts/npm-publish-no-browser.cjs",
    "publish:simple": "npm publish --access public",
    "publish:legacy": "node scripts/npm-publish-2fa.cjs",
    "publish:2fa": "node scripts/smart-npm-publish-2fa.cjs",
    "deploy": "./scripts/deploy-npm.sh",
    "deploy:dry": "./scripts/deploy-npm.sh --dry-run",
    "deploy:win": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-npm.ps1",
    "deploy:win:dry": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-npm.ps1 -DryRun"
  },
  "peerDependencies": {
    "preact": "^10.27.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "marked": "^16.1.2",
    "preact-router": "^4.1.2"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/highlight.js": "^9.12.4",
    "@types/jest-axe": "^3.5.9",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "safer-buffer": "^2.1.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "vite": "^7.1.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}